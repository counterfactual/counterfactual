{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": false,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "esnext",
    "jsx": "preserve",
    "jsxFactory": "h",
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": false
  },
  "include": [
    "lib",
    "src"
  ]
}
